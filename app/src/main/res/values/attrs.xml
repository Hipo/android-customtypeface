<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="OpenSansButton">
        <attr name="typeface" />
    </declare-styleable>

    <declare-styleable name="OpenSansEditText">
        <attr name="typeface" />
    </declare-styleable>

    <declare-styleable name="OpenSansTextView">
        <attr name="typeface" />
    </declare-styleable>


    <attr name="typeface">

        <enum name="OpenSans_Bold" value="0" />
