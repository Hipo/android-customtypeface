        <enum name="OpenSans_ExtraBoldItalic" value="3" />
        <enum name="OpenSans_Italic" value="4" />
        <enum name="OpenSans_Light" value="5" />
        <enum name="OpenSans_LightItalic" value="6" />
        <enum name="OpenSans_Regular" value="7" />
        <enum name="OpenSans_Semibold" value="8" />
        <enum name="OpenSans_SemiboldItalic" value="9" />


    </attr>

</resources>